{
    "name": "lucassabreu/feeder",
    "description": "Feeder - Feed consumer",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2",
        "feed"
    ],
    "homepage": "http://feeder-lucassabreu.rhcloud.com/",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "oauth-php/oauth-php",
                "version": "175",
                "dist": {
                    "url": "https://oauth-php.googlecode.com/files/oauth-php-175.tar.gz",
                    "type": "tar"
                },
                "source": {
                    "url": "http://oauth-php.googlecode.com/svn/",
                    "type": "svn",
                    "reference": "trunk/"
                },
                "autoload": {
                    "classmap": [
                        "library/"
                    ]
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.3.*",
        "zendframework/zftool": "dev-master",
        "zfcampus/zf-oauth2": "*"
    }
}
